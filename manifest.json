{
  "manifest_version": 2,

  "name": "Extension popup",
  "description": "This extension pastes a URL and title into a webform",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click here!"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "http://www.google.com/",
    "http://*/",
    "http://*/*"
  ]
}